<a href="/">Go back</a>
<div class="movie-detail-page">


    <img src="{{this.image}}" alt="movie cover">
    <div>
        <h1>{{this.title}}</h1>
        <h3>Director: {{this.director}} </h3>
        <h3>Stars</h3>

        <ul>
            {{#each this.stars}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <p>{{this.description}}</p>
        <p>{{this.showtimes}}</p>
    </div>
</div>